<mvc:View
    controllerName="sap.ui.ui5-practice.controller.DetailProduct"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:wt="sap.ui.ui5-practice.customcontrol"
    >
    
    <Page 
        title="{i18n>ProductdetailPageTitle}"
        showNavButton="true"
        navButtonPress=".onNavBack"
        >
        <ObjectHeader
            intro="{product>ShipperName}"
            title="{product>ProductName}"
        />
        <wt:ProductRating id="rating" class="sapUiSmallMarginBeginEnd" change=".onRatingChange"/>
    </Page>

</mvc:View>